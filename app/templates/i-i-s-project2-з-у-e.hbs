<h3 class="ui header">{{t "forms.i-i-s-project2-з-у-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field" data-test-i-i-s-project2-з-у-e-----------="true">
    <label>{{t "forms.i-i-s-project2-з-у-e.primarykey-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "guid")
      value=model.primarykey
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationObject "primarykey" "messages")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.кадНомер
      class=(if (v-get validationObject "кадНомер" "isInvalid") "error")
      label=(t "forms.i-i-s-project2-з-у-e.кадНомер-caption")
      data-test-i-i-s-project2-з-у-e---------=true
    }}
    {{flexberry-validationmessage error=(v-get validationObject "кадНомер" "messages")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.адрес
      class=(if (v-get validationObject "адрес" "isInvalid") "error")
      label=(t "forms.i-i-s-project2-з-у-e.адрес-caption")
      data-test-i-i-s-project2-з-у-e------=true
    }}
    {{flexberry-validationmessage error=(v-get validationObject "адрес" "messages")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.разрИсп
      class=(if (v-get validationObject "разрИсп" "isInvalid") "error")
      label=(t "forms.i-i-s-project2-з-у-e.разрИсп-caption")
      data-test-i-i-s-project2-з-у-e--------=true
    }}
    {{flexberry-validationmessage error=(v-get validationObject "разрИсп" "messages")}}
  </div>
</form>